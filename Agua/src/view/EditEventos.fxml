<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTextField?>
<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="240.0" prefWidth="924.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.EditEventosController">
    <stylesheets>
        <URL value="@/css/editeventos.css" />
    </stylesheets>
   <children>
      <ImageView fx:id="image_moneda" fitHeight="100.0" fitWidth="100.0" layoutX="55.0" layoutY="80.0" pickOnBounds="true" preserveRatio="true" styleClass="image_moneda" />
      <JFXTextField fx:id="txt_ValorCuota" labelFloat="true" layoutX="715.0" layoutY="75.0" onKeyReleased="#validar" prefHeight="30.0" prefWidth="150.0" promptText="Valor Q." styleClass="txt_valorCuota" />
      <JFXTextField fx:id="txt_nombreEvento" labelFloat="true" layoutX="210.0" layoutY="75.0" prefHeight="30.0" prefWidth="250.0" promptText="Nombre Evento" styleClass="txt_nombreEvento" />
      <Button fx:id="btn_editar" layoutX="362.0" layoutY="150.0" mnemonicParsing="false" onAction="#editarCuotaAction" prefHeight="30.0" prefWidth="75.0" styleClass="btn_editar" text="Editar" />
      <Button fx:id="btn_cancelar" layoutX="487.0" layoutY="150.0" mnemonicParsing="false" onAction="#cancelarAction" prefHeight="30.0" prefWidth="75.0" styleClass="btn_cancelar" text="Cancelar" />
      <DatePicker fx:id="fecha" layoutX="500.0" layoutY="75.0" prefHeight="30.0" prefWidth="175.0" promptText="Fecha" />
   </children>
</AnchorPane>

<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="AguaPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>object.Socios</class>
    <class>object.Cuotas</class>
    <class>object.PagosSocios</class>
    <class>object.Detalles</class>
    <class>object.Comprobantes</class>
    <class>object.Administradores</class>
    <class>object.Eventos</class>
    <class>object.SociosEventos</class>
    <class>object.PagosMorosos</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/proyecto_agua?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="projects"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="toor"/>
    </properties>
  </persistence-unit>
</persistence>
